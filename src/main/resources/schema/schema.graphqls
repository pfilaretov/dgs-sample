type Query {
    shows(title: String, filter: ShowFilter): [Show]
}

type Show {
    title: String
    releaseYear: Int
    actors: [Actor]
}

type Actor {
    firstName: String
    lastName: String
    role: String
}

input ShowFilter {
    actorRole: String
    genre: Genre
}

enum Genre {
    action, comedy, drama, fantasy, horror
}
